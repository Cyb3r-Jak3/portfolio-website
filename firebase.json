{
  "hosting": {
    "headers": [{
	"source":"**",
	"headers": [
	  {"key":"Content-Security-Policy","value":"default-src 'self' ; script-src 'self' ; style-src 'self' ; img-src 'self' ; font-src 'self' ; media-src 'self' ; object-src 'self'; reflected-xss block; report-uri https://jwhite.report-uri.com/r/d/csp/enforce; "},
      {"key":"X-Frame-Options","value":"SAMEORIGIN"},
      {"key":"X-XSS-Protection","value":"1; mode=block"},
      {"key":"Referrer-Policy","value":"no-referrer"},
      {"key":"Feature-Policy","value":"vibrate 'none'"},
      {"key":"Hello","value":"World"}
	]
	}],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "destination": "/index.html"
      }]
   }
}
