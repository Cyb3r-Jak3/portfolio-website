{
  "hosting": {
    "headers": [{
	"source":"**",
	"headers": [
	  {"key":"Content-Security-Policy","value":"default-src 'none' ; script-src 'self' ; style-src 'self' 'sha256-anQSeQoEnQnBulZOQkDOFf+e6xBIGmqh7M8YFT992co=' ; img-src 'self' ; font-src 'self' data; media-src 'self' ; object-src 'none'; reflected-xss block; base-uri 'self'; frame-ancestors 'none'; report-uri https://jwhite.report-uri.com/r/d/csp/enforce; "},
      {"key":"X-Frame-Options","value":"SAMEORIGIN"},
      {"key":"X-XSS-Protection","value":"1; report=https://jwhite.report-uri.com/r/d/xss/enforce"},
      {"key":"Referrer-Policy","value":"no-referrer"},
      {"key":"Feature-Policy","value":"vibrate 'none'"},
      {"key":"Hello","value":"World"},
      {"key":"Expect-Staple","value":"max-age=31536000; report-uri='https://jwhite.report-uri.com/r/d/staple/enforce'; includeSubDomains; preload"}
	]
	}],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "destination": "/index.html"
      }]
   }
}
