{
  "hosting": {
    "cleanUrls": true,
    "headers": [{
	"source":"**",
	"headers": [
	  {"key":"Content-Security-Policy","value":"default-src 'none' ; script-src 'self' ; style-src 'self' 'unsafe-inline' 'sha256-anQSeQoEnQnBulZOQkDOFf+e6xBIGmqh7M8YFT992co=' ajax.cloudflare.com; img-src 'self' ; font-src 'self'; media-src 'self' ; object-src 'none'; base-uri 'self'; frame-ancestors 'none'; report-uri https://jwhite.report-uri.com/r/d/csp/enforce; "},
    {"key":"X-Frame-Options","value":"SAMEORIGIN"},
    {"key":"X-XSS-Protection","value":"1; report=https://jwhite.report-uri.com/r/d/xss/enforce"},
    {"key":"Referrer-Policy","value":"no-referrer"},
    {"key":"Hello","value":"World"},
    {"key":"Expect-Staple","value":"max-age=31536000; report-uri='https://jwhite.report-uri.com/r/d/staple/enforce'; includeSubDomains; preload"},
    {"key":"Feature-Policy","value":"Feature-Policy: vibrate 'none';"},
    {"key":"Strict-Transport-Secuirty","value":"max-age=31536000; includeSubDomains; preload"}
	]
	}],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "destination": "/index.html"
      }]
   }
}
