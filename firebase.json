{
  "hosting": {
    "headers": [{
	"source":"**",
	"headers": [
	  {"key":"Content-Security-Policy","value":"script-src 'self'"}
	]
	}],
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**/(*.css|*.js|*.map|*.jpg|*.gif|*.png|*.php)",
        "destination": "/index.html"
      }]
   }
}
